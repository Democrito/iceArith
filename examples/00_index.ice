                      "range": "[31:0]",
                      "size": 32
                    },
                    {
                      "name": "start"
                    }
                  ],
                  "out": [
                    {
                      "name": "busy"
                    },
                    {
                      "name": "cociente",
                      "range": "[31:0]",
                      "size": 32
                    },
                    {
                      "name": "resto",
                      "range": "[31:0]",
                      "size": 32
                    }
                  ]
                }
